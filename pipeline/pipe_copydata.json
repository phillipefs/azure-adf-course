{
	"name": "pipe_copydata",
	"properties": {
		"activities": [
			{
				"name": "Copy_Excel_to_CSV",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"additionalColumns": [
							{
								"name": "data_carga",
								"value": {
									"value": "@formatDateTime(utcNow(), 'dd-MM-yyyy')",
									"type": "Expression"
								}
							}
						],
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_excel_fixo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_generico_csv",
						"type": "DatasetReference",
						"parameters": {
							"container_name": "csvfiles",
							"file_name": "csvCor.csv",
							"separator_name": ";"
						}
					}
				]
			}
		],
		"folder": {
			"name": "adf_02_activities"
		},
		"annotations": [],
		"lastPublishTime": "2023-02-23T22:25:37Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}